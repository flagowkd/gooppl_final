<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="goo.ad">
	<insert id="startAD" parameterType="goo.ad.model.AdDTO">
	insert into
		ad_info
	values(
		ad_info_idx.nextval,
		#{owner_idx},
		#{imp_uid},
		sysdate
	)
	</insert>
</mapper>