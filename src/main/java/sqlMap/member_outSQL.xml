<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="goo.member_out">
  	<select id="memberOutList" resultType="goo.memberout.model.MemberOutDTO">
  		SELECT
  			*
  		FROM
  			member_out
  		ORDER BY out_no desc
  	</select>
  	<insert id="insertMemberOut">
  		insert into member_out values(member_out_idx.nextval,#{goo_id},#{out_reason},sysdate)
  	</insert>
  </mapper>